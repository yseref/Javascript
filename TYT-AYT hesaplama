function Edbayt(){
    while(true){
let AYTedbdogru = Number(prompt("Ayt Edebiyat tesintden yaptÄ±ÄŸÄ±nÄ±z doÄŸru sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(AYTedbdogru<=24){
let AytedbYanlis = Number(prompt("AYT Edebiyat testinden yaptÄ±ÄŸÄ±nÄ±z yanlÄ±ÅŸ sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(AYTedbdogru+AytedbYanlis<=24){
console.log("AYT Edebiyat tesindeki doÄŸru sayÄ±nÄ±z: " + AYTedbdogru  + "AYT Edebiyat tesindeki yanlÄ±ÅŸ sayÄ±nÄ±z: " + AytedbYanlis)
return (AYTedbdogru-(AytedbYanlis/4));
}else{hata()}
}else{hata()}
    }}


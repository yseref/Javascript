    }}


    
    function dinnet(){
    while(true){
let AYTdindogru = Number(prompt("Ayt Din Kültürü ve Ahlak Bilgisi tesintden yaptığınız doğru sayısını yazınız"))
if(AYTdindogru<=6){
let AytdinYanlis = Number(prompt("AYT Din Kültürü ve Ahlak Bilgisi testinden yaptığınız yanlış sayısını yazınız"))
if(AYTtar1dogru+Ayttar1Yanlis<=6){
console.log("AYT Din Kültürü ve Ahlak Bilgisi tesindeki doğru sayınız: " + AYTdindogru  + "AYT Din Kültürü ve Ahlak Bilgisi tesindeki yanlış sayınız: " + AytdinYanlis)
return (AYTdindogru-(AytdinYanlis/4));
}else{hata()}
}else{hata()}
    }}

    function felsefeAyt(){
    while(true){
let AYTfelsefedogru = Number(prompt("Ayt Felsefe Grubu tesintden yaptığınız doğru sayısını yazınız"))
if(AYTfelsefedogru<=12){
let AytfelsefeYanlis = Number(prompt("AYT Felsefe Grubu testinden yaptığınız yanlış sayısını yazınız"))
if(AYTfelsefedogru+AytfelsefeYanlis<=12){
console.log("AYT Felsefe Grubu tesindeki doğru sayınız: " + AYTfelsefedogru  + "AYT Felsefe Grubu tesindeki yanlış sayınız: " + AytfelsefeYanlis)
return (AYTfelsefedogru-(AytfelsefeYanlis/4));
}else{hata()}
}else{hata()}
    }}







function dilydt(){
    while(true){
let ydtdogru = Number(prompt("YDT Edebiyat tesintden yaptığınız doğru sayısını yazınız"))
if(ydtdogru<=40){
let ydtYanlis = Number(prompt("YDT Edebiyat testinden yaptığınız yanlış sayısını yazınız"))
if(ydtdogru+ydtYanlis<=40){
console.log("YDT tesindeki doğru sayınız: " + ydtdogru  + "YDT tesindeki yanlış sayınız: " + ydtYanlis)
return (ydtdogru-(ydtYanlis/4));
}else{hata()}
}else{hata()}
}}

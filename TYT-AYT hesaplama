function DogruYanlisNetmattrtyt(ders){
    while(true){
let dogru = Number(prompt( ders +" testindeki doÄŸru sayÄ±nÄ±zÄ± giriniz"))
if(dogru<=40){
let yanlis = Number(prompt( ders +  " testindeki yanlÄ±ÅŸ sayÄ±nÄ±zÄ± giriniz"))
if(dogru+yanlis<=40){
    console.log(ders + " DoÄŸru sayÄ±nÄ±z: " + dogru + " " + ders + " YanlÄ±ÅŸ sayÄ±nÄ±z: " + yanlis )
return (dogru-(yanlis/4));
}else{
alert("YanlÄ±ÅŸ deÄŸer girdiniz,lÃ¼tfen tekrar deneyin!")

}
    }else {hata()}
}}





function DogruYanlisNetsosfen (ders){
while(true){
let dogru = Number(prompt( ders +" testindeki doÄŸru sayÄ±nÄ±zÄ± giriniz"))
if(dogru<=20){
let yanlis = Number(prompt( ders +  " testindeki yanlÄ±ÅŸ sayÄ±nÄ±zÄ± giriniz"))
if(dogru+yanlis<=20){
console.log (ders + " DoÄŸru sayÄ±nÄ±z: " + dogru + " " + ders + " YanlÄ±ÅŸ sayÄ±nÄ±z: " + yanlis )
return dogru-(yanlis/4);
}else{
hata()
}
} else {hata ()}
}
}

function hata(){
alert("YanlÄ±ÅŸ deÄŸer girdiniz,lÃ¼tfen tekrar deneyiniz!")


}


function OBPBÄ°LGÄ°lendirme(){

let obpBilgilendirme = `
ðŸŽ“ OBP (OrtaÃ¶ÄŸretim BaÅŸarÄ± PuanÄ±) HakkÄ±nda Bilgilendirme

âœ… OBP, lise baÅŸarÄ±nÄ±za gÃ¶re YKS puanÄ±nÄ±za eklenen katkÄ±dÄ±r.  
âœ… OBP yalnÄ±zca bazÄ± durumlarda yarÄ±ya dÃ¼ÅŸer (kÄ±rÄ±lÄ±r).

ðŸ“Œ OBP Hangi Durumlarda KÄ±rÄ±lÄ±r?
1. AynÄ± alandaki bÃ¶lÃ¼mÃ¼ tercih edip kazanÄ±rsanÄ±z  
   (Ã–rn: BiliÅŸim lisesi â†’ Bilgisayar MÃ¼hendisliÄŸi).
2. Daha Ã¶nce bir Ã¶rgÃ¼n Ã¼niversite programÄ±na (Ã¶nlisans/lisans) yerleÅŸtiyseniz.  
   â†’ KayÄ±t yaptÄ±rmasanÄ±z bile OBP kÄ±rÄ±lÄ±r.

ðŸ“Œ OBP Hangi Durumlarda KÄ±rÄ±lmaz?
- AÃ§Ä±kÃ¶ÄŸretim kazanÄ±rsanÄ±z.
- Sadece sÄ±nava girip hiÃ§bir yere yerleÅŸmezseniz.
- Alan dÄ±ÅŸÄ± bÃ¶lÃ¼mleri yazarsanÄ±z.

â„¹ï¸ Ã–zet: OBP sadece daha Ã¶nce yerleÅŸtiyseniz ya da alanÄ±nÄ±zla aynÄ± bÃ¶lÃ¼mÃ¼ tekrar yazarsanÄ±z kÄ±rÄ±lÄ±r.
`;

alert(obpBilgilendirme)}

function MatAyt(){
    while(true){
let AYTmatdogru = Number(prompt("Ayt matematik tesintden yaptÄ±ÄŸÄ±nÄ±z doÄŸru sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(AYTmatdogru<=40){
let AytMatYanlis = Number(prompt("AYT matematik testinden yaptÄ±ÄŸÄ±nÄ±z yanlÄ±ÅŸ sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(AYTmatdogru+AytMatYanlis<=40){
console.log("AYT matematik tesindeki doÄŸru sayÄ±nÄ±z: " + AYTmatdogru  + "AYT matematik tesindeki yanlÄ±ÅŸ sayÄ±nÄ±z: " + AytMatYanlis)
return (AYTmatdogru-(AytMatYanlis/4));
}else{hata()}
}else{hata()}
    }}


    function Fiziknet(){
while(true){
let Fizikdogru = Number(prompt("AYT Fizik testinde yaptÄ±ÄŸÄ±nÄ±z doÄŸru sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(Fizikdogru<=14){
let Fizikyanlis = Number(prompt("AYT Fizik testinde yaptÄ±ÄŸÄ±nÄ±z yanlÄ±ÅŸ sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(Fizikdogru+Fizikyanlis<=14){
console.log("AYT Fizik tesindeki doÄŸru sayÄ±nÄ±z: " + Fizikdogru  + "AYT Fizik tesindeki yanlÄ±ÅŸ sayÄ±nÄ±z: " + Fizikyanlis)
return (Fizikdogru-(Fizikyanlis/4));
}else{hata()}


}else{hata()}


}



    }
    function kimyaaytnet(){
        while (true){
let aytkimyadogru = Number(prompt("AYT Kimya testinde yaptÄ±ÄŸÄ±nÄ±z doÄŸru sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(aytkimyadogru<=13){
let aytkimyayanlis = Number(prompt("AYT Kimya testinde yaptÄ±ÄŸÄ±nÄ±z yanlÄ±ÅŸ sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(aytkimyadogru+aytkimyayanlis<=13){
console.log("AYT Kimya tesindeki doÄŸru sayÄ±nÄ±z: " + aytkimyadogru  + "AYT Kimya tesindeki yanlÄ±ÅŸ sayÄ±nÄ±z: " + aytkimyayanlis)
return (aytkimyadogru-(aytkimyayanlis/4))

}else{(hata())}


}else{(hata())}


        }}


        function Biyolojiaytnet(){
while(true){
let Biyoaytdogru = Number(prompt("AYT Biyoloji testinde yaptÄ±ÄŸÄ±nÄ±z doÄŸru sayÄ±sÄ±nÄ± yazÄ±nÄ±z"))
if(Biyoaytdogru<=13){
let Biyoaytyanlis = Number(prompt(("AYT Biyoloji testinde yaptÄ±ÄŸÄ±nÄ±z yanlÄ±ÅŸ sayÄ±sÄ±nÄ± yazÄ±nÄ±z")))
if(Biyoaytdogru+Biyoaytyanlis<=13){
console.log("AYT Biyoloji tesindeki doÄŸru sayÄ±nÄ±z: " + Biyoaytdogru  + "AYT Biyoloji tesindeki yanlÄ±ÅŸ sayÄ±nÄ±z: " + Biyoaytyanlis)
return (Biyoaytdogru-(Biyoaytyanlis/4));



}else{hata()}

}else{hata()}



}



        }


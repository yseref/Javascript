        }

    function yanlisyazi (ders){

let art = "AYT " + ders + "testindeki yanlış sayınız"


    }
function Edbayt(){
    while(true){
let AYTedbdogru = Number(prompt("Ayt Edebiyat tesintden yaptığınız doğru sayısını yazınız"))
if(AYTedbdogru<=24){
let AytedbYanlis = Number(prompt("AYT Edebiyat testinden yaptığınız yanlış sayısını yazınız"))
if(AYTedbdogru+AytedbYanlis<=24){
console.log("AYT Edebiyat tesindeki doğru sayınız: " + AYTedbdogru  + "AYT Edebiyat tesindeki yanlış sayınız: " + AytedbYanlis)
return (AYTedbdogru-(AytedbYanlis/4));
}else{hata()}
}else{hata()}
    }}

    function tar1net(){
    while(true){
let AYTtar1dogru = Number(prompt("Ayt Tarih-1 tesintden yaptığınız doğru sayısını yazınız"))
if(AYTtar1dogru<=10){
let Ayttar1Yanlis = Number(prompt("AYT Tarih-1 testinden yaptığınız yanlış sayısını yazınız"))
if(AYTtar1dogru+Ayttar1Yanlis<=10){
console.log("AYT Tarih-1 tesindeki doğru sayınız: " + AYTtar1dogru  + "AYT Tarih-1 tesindeki yanlış sayınız: " + Ayttar1Yanlis)
return (AYTtar1dogru-(Ayttar1Yanlis/4));
}else{hata()}
}else{hata()}
    }}


        function cog1net(){
    while(true){
let AYTcog1dogru = Number(prompt("Ayt Coğrafya-1 tesintden yaptığınız doğru sayısını yazınız"))
if(AYTcog1dogru<=6){
let Aytcog1Yanlis = Number(prompt("AYT Coğrafya-1 testinden yaptığınız yanlış sayısını yazınız"))
if(AYTcog1dogru+Aytcog1Yanlis<=6){
console.log("AYT Coğrafya-1 tesindeki doğru sayınız: " + AYTcog1dogru  + "AYT Coğrafya-1 tesindeki yanlış sayınız: " + Aytcog1Yanlis)
return (AYTcog1dogru-(Aytcog1Yanlis/4));
}else{hata()}
}else{hata()}
    }}


    
        function cog2net(){
    while(true){
let AYTcog2dogru = Number(prompt("Ayt Coğrafya-2 tesintden yaptığınız doğru sayısını yazınız"))
if(AYTcog2dogru<=11){
let Aytcog2Yanlis = Number(prompt("AYT Coğrafya-2 testinden yaptığınız yanlış sayısını yazınız"))

function DogruYanlisNetmattrtyt(ders){
    while(true){
let dogru = Number(prompt( ders +" testindeki doğru sayınızı giriniz"))
if(dogru<=40){
let yanlis = Number(prompt( ders +  " testindeki yanlış sayınızı giriniz"))
if(dogru+yanlis<=40){
    console.log(ders + " Doğru sayınız: " + dogru + " " + ders + " Yanlış sayınız: " + yanlis )
return (dogru-(yanlis/4));
}else{
alert("Yanlış değer girdiniz,lütfen tekrar deneyin!")

}
    }else {hata()}
}}





function DogruYanlisNetsosfen (ders){
while(true){
let dogru = Number(prompt( ders +" testindeki doğru sayınızı giriniz"))
if(dogru<=20){
let yanlis = Number(prompt( ders +  " testindeki yanlış sayınızı giriniz"))
if(dogru+yanlis<=20){
console.log (ders + " Doğru sayınız: " + dogru + " " + ders + " Yanlış sayınız: " + yanlis )
return dogru-(yanlis/4);
}else{
hata()
}
} else {hata ()}
}
}

function hata(){
alert("Yanlış değer girdiniz,lütfen tekrar deneyiniz!")


}


function OBPBİLGİlendirme(){

let obpBilgilendirme = `
🎓 OBP (Ortaöğretim Başarı Puanı) Hakkında Bilgilendirme

✅ OBP, lise başarınıza göre YKS puanınıza eklenen katkıdır.  
✅ OBP yalnızca bazı durumlarda yarıya düşer (kırılır).

📌 OBP Hangi Durumlarda Kırılır?
1. Aynı alandaki bölümü tercih edip kazanırsanız  
   (Örn: Bilişim lisesi → Bilgisayar Mühendisliği).
2. Daha önce bir örgün üniversite programına (önlisans/lisans) yerleştiyseniz.  
   → Kayıt yaptırmasanız bile OBP kırılır.

📌 OBP Hangi Durumlarda Kırılmaz?
- Açıköğretim kazanırsanız.
- Sadece sınava girip hiçbir yere yerleşmezseniz.
- Alan dışı bölümleri yazarsanız.

ℹ️ Özet: OBP sadece daha önce yerleştiyseniz ya da alanınızla aynı bölümü tekrar yazarsanız kırılır.
`;

alert(obpBilgilendirme)}

function MatAyt(){
    while(true){
let AYTmatdogru = Number(prompt("Ayt matematik tesintden yaptığınız doğru sayısını yazınız"))
if(AYTmatdogru<=40){
let AytMatYanlis = Number(prompt("AYT matematik testinden yaptığınız yanlış sayısını yazınız"))
if(AYTmatdogru+AytMatYanlis<=40){
console.log("AYT matematik tesindeki doğru sayınız: " + AYTmatdogru  + "AYT matematik tesindeki yanlış sayınız: " + AytMatYanlis)
return (AYTmatdogru-(AytMatYanlis/4));
}else{hata()}
}else{hata()}
    }}


    function Fiziknet(){
while(true){
let Fizikdogru = Number(prompt("AYT Fizik testinde yaptığınız doğru sayısını yazınız"))
if(Fizikdogru<=14){
let Fizikyanlis = Number(prompt("AYT Fizik testinde yaptığınız yanlış sayısını yazınız"))
if(Fizikdogru+Fizikyanlis<=14){
console.log("AYT Fizik tesindeki doğru sayınız: " + Fizikdogru  + "AYT Fizik tesindeki yanlış sayınız: " + Fizikyanlis)
return (Fizikdogru-(Fizikyanlis/4));
}else{hata()}


}else{hata()}


}



    }
    function kimyaaytnet(){
        while (true){
let aytkimyadogru = Number(prompt("AYT Kimya testinde yaptığınız doğru sayısını yazınız"))
if(aytkimyadogru<=13){
let aytkimyayanlis = Number(prompt("AYT Kimya testinde yaptığınız yanlış sayısını yazınız"))
if(aytkimyadogru+aytkimyayanlis<=13){
console.log("AYT Kimya tesindeki doğru sayınız: " + aytkimyadogru  + "AYT Kimya tesindeki yanlış sayınız: " + aytkimyayanlis)
return (aytkimyadogru-(aytkimyayanlis/4))

}else{(hata())}


}else{(hata())}


        }}


        function Biyolojiaytnet(){
while(true){
let Biyoaytdogru = Number(prompt("AYT Biyoloji testinde yaptığınız doğru sayısını yazınız"))
if(Biyoaytdogru<=13){
let Biyoaytyanlis = Number(prompt(("AYT Biyoloji testinde yaptığınız yanlış sayısını yazınız")))
if(Biyoaytdogru+Biyoaytyanlis<=13){
console.log("AYT Biyoloji tesindeki doğru sayınız: " + Biyoaytdogru  + "AYT Biyoloji tesindeki yanlış sayınız: " + Biyoaytyanlis)
return (Biyoaytdogru-(Biyoaytyanlis/4));



}else{hata()}

}else{hata()}



}



        }


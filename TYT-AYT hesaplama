console.log("AYT Coğrafya-2 tesindeki doğru sayınız: " + AYTcog2ogru  + "AYT Coğrafya-1 tesindeki yanlış sayınız: " + Aytcog2Yanlis)
return (AYTcog2dogru-(Aytcog2Yanlis/4));
}else{hata()}
}else{hata()}
    }}


    
    function tar2net(){
